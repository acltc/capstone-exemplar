                <!-- Main menu -->
                <li><a href="/meetings">Meetings</a>
                  <!-- Submenu -->
                  <ul>
                    <li><a href="index.html">Main Home</a></li>
                    <li><a href="index-bootstrap-carousel.html">Home Carousel</a></li>
